extends layout.pug 



block content 
    h1 saved playlist 
    

    table
        each p in playlist
            tr 
                - var url = p.external_urls.spotify;
                - var name =  p.name
                td #{p.name}
                td: <iframe style="border-radius:12px" src=#{url} width="100%" height="352" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>